{
    "name": "ha-sip",
    "version": "1.4",
    "slug": "ha-sip",
    "url": "https://github.com/arnonym/ha-plugins",
    "description": "Home-Assistant SIP Gateway",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "image": "agellhaus/{arch}-ha-sip",
    "startup": "application",
    "boot": "auto",
    "options": {
        "sip": {
            "registrar_uri": "sip:fritz.box",
            "port": 5060,
            "id_uri": "sip:homeassistant@fritz.box",
            "realm": "*",
            "user_name": "homeassistant",
            "password": "secure"
        },
        "tts": {
            "platform": "google_translate",
            "language": "en"
        },
        "webhook": {
            "id": "sip_call_webhook_id"
        }
    },
    "schema": {
        "sip": {
            "registrar_uri": "str",
            "port": "int",
            "id_uri": "str",
            "realm": "str",
            "user_name": "str",
            "password": "str"
        },
        "tts": {
            "platform": "str",
            "language": "str"
        },
        "webhook": {
            "id": "str"
        }
    },
    "host_network": true,
    "homeassistant_api": true,
    "hassio_api": false,
    "stdin": true
}
