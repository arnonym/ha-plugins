{
    "name": "ha-sip",
    "version": "5.3",
    "slug": "ha-sip",
    "url": "https://github.com/arnonym/ha-plugins",
    "description": "Home-Assistant SIP Gateway",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "image": "agellhaus/{arch}-ha-sip",
    "init": "false",
    "startup": "application",
    "boot": "auto",
    "options": {
        "sip_global": {
            "port": 5060,
            "log_level": 5,
            "name_server": "",
            "cache_dir": "",
            "global_options": ""
        },
        "sip": {
            "enabled": true,
            "registrar_uri": "sip:fritz.box",
            "id_uri": "sip:homeassistant@fritz.box",
            "realm": "*",
            "user_name": "homeassistant",
            "password": "secure",
            "answer_mode": "listen",
            "settle_time": 1,
            "incoming_call_file": "",
            "options": ""
        },
        "sip_2": {
            "enabled": false,
            "registrar_uri": "sip:fritz.box",
            "id_uri": "sip:anotheruser@fritz.box",
            "realm": "*",
            "user_name": "anotheruser",
            "password": "secret",
            "answer_mode": "listen",
            "settle_time": 1,
            "incoming_call_file": "",
            "options": ""
        },
        "sip_3": {
            "enabled": false,
            "registrar_uri": "sip:192.168.178.10",
            "id_uri": "",
            "realm": "*",
            "user_name": "",
            "password": "",
            "answer_mode": "listen",
            "settle_time": 1,
            "incoming_call_file": "",
            "options": ""
        },
        "tts": {
            "engine_id": "",
            "platform": "",
            "language": "",
            "voice": "",
            "debug_print": false
        },
        "webhook": {
            "id": "sip_call_webhook_id"
        }
    },
    "schema": {
        "sip_global": {
            "port": "int",
            "log_level": "int",
            "name_server": "str",
            "cache_dir": "str",
            "global_options": "str"
        },
        "sip": {
            "enabled": "bool",
            "registrar_uri": "str",
            "id_uri": "str",
            "realm": "str",
            "user_name": "str",
            "password": "str",
            "answer_mode": "list(listen|accept)",
            "settle_time": "float",
            "incoming_call_file": "str",
            "options": "str"
        },
        "sip_2": {
            "enabled": "bool",
            "registrar_uri": "str",
            "id_uri": "str",
            "realm": "str",
            "user_name": "str",
            "password": "str",
            "answer_mode": "list(listen|accept)",
            "settle_time": "float",
            "incoming_call_file": "str",
            "options": "str"
        },
        "sip_3": {
            "enabled": "bool",
            "registrar_uri": "str",
            "id_uri": "str",
            "realm": "str",
            "user_name": "str",
            "password": "str",
            "answer_mode": "list(listen|accept)",
            "settle_time": "float",
            "incoming_call_file": "str",
            "options": "str"
        },
        "tts": {
            "platform": "str?",
            "engine_id": "str?",
            "language": "str",
            "voice": "str?",
            "debug_print": "bool?"
        },
        "webhook": {
            "id": "str"
        }
    },
    "host_network": true,
    "homeassistant_api": true,
    "hassio_api": false,
    "stdin": true,
    "map": [
        {
            "type": "homeassistant_config",
            "read_only": false,
            "path": "/config"
        },
        {
            "type": "media",
            "read_only": false,
            "path": "/media"
        }
    ]
}
